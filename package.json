  "scripts": {
    "sass:process": "node-sass src/assets/scss -o dist/assets/css",
    "sass:watch": "node-sass --watch src/assets/scss -o dist/assets/css",
    "serve": "concurrently \"npx eleventy --serve\" \"npm run sass:watch\"",
    "production": "npx eleventy && npm run sass:process"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "node-sass": "^4.13.1",
    "concurrently": "^5.1.0"
  }
}
